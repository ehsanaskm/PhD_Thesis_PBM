FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      moment.4.populationBalance;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 403.4287934927351;

boundaryField
{

    inletTop
    {
        type            fixedValue;
        value           uniform 1;
    }
    inletBottom
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    reactorWalls
    {
        type            zeroGradient;
    }
    spargerWalls
    {
        type            zeroGradient;
    }
    impellerWalls
    {
        type            zeroGradient;
    }
}


