FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      epsilon.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    stator
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
    rotor
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
    spargerInlet
    {
        type            fixedValue;
        value           $internalField;
    }
    spargerWall
    {
        type            epsilonWallFunction;
        value           uniform 0.01;
    }
    top
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      $internalField;
        value           $internalField;
    }
}


// ************************************************************************* //
