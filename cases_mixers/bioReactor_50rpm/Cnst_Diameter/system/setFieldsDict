FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 0.999999
);

regions
(
    boxToCell
    {
        box (-0.0625 -0.0625 0) (0.0625 0.0625 0.1666);
        fieldValues
        (
            volScalarFieldValue alpha.air 1e-3
        );
    }
);


