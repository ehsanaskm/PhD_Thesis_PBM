FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Twater;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 0 0 1 0 0 0];

internalField       uniform 300;

boundaryField
{

    outlet
    {

        type               inletOutlet;
        phi                phi.water;
        inletValue         $internalField;
        value              $internalField;
    }
    inletTop
    {

        type               fixedValue;
        value              $internalField;
    }

    inletBottom
    {

        type               fixedValue;
        value              $internalField;
    }

    shaft
    {
        type            zeroGradient;
    }
    sparger
    {
        type            zeroGradient;
    }
    vessel
    {
        type            zeroGradient;
    }
    impeller
    {
        type            zeroGradient;
    }
        heater
    {
        type            zeroGradient;
    }
    
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
